#!/bin/bash

source ${POS_SCRIPT_PRIV}/utils.sh

if [[ "$1" == "--help-brief" ]]
then
    echo "Clear  to the current SshDispatcher"
    exit
fi

if [[ "$1" == "" || "$1" == "help" || "$1" == "-h" || "$1" == "--help" ]]
then
    echo help #!TODO
    exit
fi


one_host=$1
move_to_sshdistroot
zz
echo "Clear log"
echo "" > log

echo "Clear out"
echo "" | tee $(echo out/*)

echo "Clear err"
echo "" | tee $(echo err/*)



